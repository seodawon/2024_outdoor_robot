<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dddd"
    tools:context=".login_customer"
    tools:ignore="ExtraText">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/top2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/textView5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_weight="0.5"
        tools:layout_editor_absoluteX="0dp">
        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:padding="1dp"
            android:scaleType="centerCrop"
            android:src="@drawable/back"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/imageView4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="53dp"
            android:scaleType="fitStart"
            android:src="@drawable/image"
            app:layout_constraintLeft_toRightOf="@id/imageButton4"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <LinearLayout
        android:id="@+id/v2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintVertical_weight="2">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/textView17"
                android:layout_marginLeft="10dp"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:textAlignment="center"
                android:gravity="center"
                android:text="아이디"
                android:textSize="19dp"/>
            <EditText
                android:id="@+id/id"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6.2"
                android:ems="10"
                android:layout_marginRight="26dp"
                android:hint="아이디"
                android:inputType="text"
                android:textSize="24dp"
                tools:ignore="HardcodedText,MissingConstraints"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/textView9"
                android:layout_width="0dp"
                android:textSize="19dp"
                android:textAlignment="center"
                android:gravity="center"
                android:layout_marginLeft="8dp"
                android:layout_weight="3"
                android:layout_height="match_parent"
                android:text="비밀번호"/>
            <EditText
                android:id="@+id/password"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:ems="10"
                android:layout_marginRight="26dp"
                android:layout_weight="9"
                android:hint="비밀번호"
                android:inputType="text"
                android:textSize="24dp"
                tools:ignore="MissingConstraints"
                android:layout_gravity="center_horizontal"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/v3"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintVertical_weight="0.4"
        android:layout_marginBottom="20dp">
        <ImageButton
            android:id="@+id/imageButton16"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:contentDescription="TODO"
            android:padding="1dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/kakao"
            tools:ignore="MissingConstraints"
            android:background="@android:color/transparent"
            android:layout_marginRight="30dp"
            android:layout_weight="1"/>
        <ImageButton
            android:id="@+id/imageButton15"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:contentDescription="TODO"
            android:padding="1dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/nave"
            tools:ignore="MissingConstraints"
            android:background="@android:color/transparent"
            android:layout_weight="1"/>
        <ImageButton
            android:id="@+id/imageButton17"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:contentDescription="TODO"
            android:padding="1dp"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/gold"
            tools:ignore="MissingConstraints"
            android:background="@android:color/transparent"
            android:layout_marginLeft="30dp"
            android:layout_weight="1"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/v4"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_weight="0.4">
        <android.widget.Button
            android:id="@+id/button6"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="ID 찾기"
            android:textColor="#000000"
            tools:ignore="MissingConstraints"
            android:textSize="24dp"
            android:layout_weight="1"/>
        <android.widget.Button
            android:id="@+id/button4"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="PW 찾기"
            android:textColor="#000000"
            tools:ignore="MissingConstraints"
            android:layout_weight="1"
            android:textSize="24dp"/>
        <android.widget.Button
            android:id="@+id/btn_register1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="회원가입"
            android:textSize="24dp"
            android:textColor="#000000"
            android:layout_weight="1"/>
    </LinearLayout>
    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#992A42"
        android:text="로그인"
        android:textColor="#FFFFFF"
        android:textSize="32dp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:gravity="center"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintBottom_toTopOf="@id/v2"
        app:layout_constraintLeft_toRightOf="@id/a"
        app:layout_constraintRight_toLeftOf="@id/a1"
        app:layout_constraintTop_toBottomOf="@id/top2"
        app:layout_constraintVertical_weight="0.3"/>
    <View
        android:id="@+id/a"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:background="#853347"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintRight_toLeftOf="@+id/textView5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"/>
    <View
        android:id="@+id/a1"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:background="#853347"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintRight_toRightOf="parent"/>
    <View
        android:id="@+id/pay1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintRight_toLeftOf="@+id/button3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button3"
        app:layout_constraintBottom_toBottomOf="@+id/button3"/>
    <View
        android:id="@+id/pay2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/button3"
        app:layout_constraintTop_toTopOf="@+id/button3"
        app:layout_constraintBottom_toBottomOf="@+id/button3"
        app:layout_constraintRight_toRightOf="parent"/>
    <android.widget.Button
        android:id="@+id/button3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#CF7488"
        android:text="로그인"
        android:textColor="#FFFFFF"
        android:textSize="24dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintHorizontal_weight="1.7"
        app:layout_constraintBottom_toTopOf="@id/v4"
        app:layout_constraintLeft_toRightOf="@id/pay1"
        app:layout_constraintRight_toLeftOf="@id/pay2"
        app:layout_constraintTop_toBottomOf="@id/v3"
        app:layout_constraintVertical_weight="0.4"/>
    <View
        android:id="@+id/b3"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:background="#853347"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.6"/>
    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="or" android:textSize="16dp"
        android:textAlignment="center"
        android:gravity="center"
        app:layout_constraintHorizontal_weight="0.6"
        app:layout_constraintTop_toBottomOf="@+id/v2"
        app:layout_constraintLeft_toRightOf="@id/b"
        app:layout_constraintRight_toLeftOf="@id/b1"
        app:layout_constraintBottom_toTopOf="@+id/v3"
        app:layout_constraintVertical_weight="0.3"/>
    <View
        android:id="@+id/b"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:background="@drawable/dash_line"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintRight_toLeftOf="@+id/textView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView"
        app:layout_constraintBottom_toBottomOf="@+id/textView"/>
    <View
        android:id="@+id/b1"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:background="@drawable/dash_line"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textView"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintRight_toRightOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>